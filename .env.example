# Remora Contract Deployment Sample Env
# Copy this file to .env and fill in your values
# NEVER commit .env to git!

# =============================================================================
# RPC / Fork
# =============================================================================
MAINNET_RPC_URL=https://mainnet.infura.io/v3/{$key}

# Local Anvil / RPC endpoint used by scripts
RPC_URL=http://127.0.0.1:8545

# =============================================================================
# Deploy Script Inputs
# =============================================================================

# Deployer private key (0x-prefixed hex, can use Anvil default)
PRIVATE_KEY=0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80

# Agent address allowed to manage positions (defaults to deployer)
AGENT_ADDRESS=

# Existing factory address for production-like deploys
# If empty, DeployVault.s.sol will deploy a new factory
FACTORY_ADDRESS=
